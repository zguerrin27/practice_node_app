<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/head'); %>
  </head>
  <body class="container">

    <header>
      <%- include('../partials/header'); %>
    </header>
    <br>
    <br>
    <br>


    <main>

      <h1>New Person</h1>

      <form action="/post" method="post">
        <div class="form-group">
          <label for="title">Wiki Title</label>
          <input type="text" class="form-control" name="title" aria-describedby="titleHelp" placeholder="Enter Title">
        </div>
        <div class="form-group">
          <label for="description">Wiki Body</label>
          <textarea rows="4" class="form-control" name="body" placeholder="Enter Body"></textarea>
        </div>
        <div class="form-check">
          <input class="form-check-input" type="radio" name="private" value="false" checked>
          <label class="form-check-label" for="private">Public (default)</label>
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
      </form>

    </main>

   
  </body>

</html>